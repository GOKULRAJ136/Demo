public class If {
    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
        System.out.print("Input number: ");
        int n = in.nextInt();
        if (n<0) {
            System.out.println("Negative");
        } else if (n==0) {
            System.out.println("Zero");
        } else {
            System.out.println("Positive");
        }
    }
}
